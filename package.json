    "detox": {
        "configurations": {
            "ios.sim.debug": {
                "binaryPath": "ios/build/NexusAuroraPlatform/Build/Products/Debug-iphonesimulator/NexusAuroraPlatform.app",
                "build": "xcodebuild -project ios/NexusAuroraPlatform.xcworkspace -scheme NexusAuroraPlatform -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
                "type": "ios.simulator",
                "device": {
                    "type": "iPhone 11"
                }
            },
            "android.emu.debug": {
                "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
                "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
                "type": "android.emulator",
                "device": {
                    "avdName": "Nexus_5X_API_29"
                }
            },
            "android.emu.release": {
                "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
                "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
                "type": "android.emulator",
                "device": {
                    "avdName": "Nexus_5X_API_29"
                }
            }
        },
        "test-runner": "jest"
    }
